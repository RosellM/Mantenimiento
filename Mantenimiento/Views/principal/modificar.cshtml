@model Mantenimiento.Models.UsuarioModel
@{
    Layout = null;
}
@using (@Html.BeginForm("modificarUsuario", "principal"))
{
    @Html.HiddenFor(model=>model.id)
    @Html.LabelFor(model => model.nombre)
    @Html.TextBoxFor(model => model.nombre, new { @class = "form-control", required = "required" })

    @Html.LabelFor(model => model.nombreApellidoMaterno)
    @Html.TextBoxFor(model => model.nombreApellidoMaterno, new { @class = "form-control", required = "required" })

    @Html.LabelFor(model => model.nombreApellidoPaterno)
    @Html.TextBoxFor(model => model.nombreApellidoPaterno, new { @class = "form-control", required = "required" })

    <input type="submit" value="Modificar" class="btn btn-success btn-block margin-bottom-lg" />
}
